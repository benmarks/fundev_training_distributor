<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Training_Distro>
            <version>0.10.0.1</version>
        </Training_Distro>
    </modules>
    <adminhtml>
        <layout>
            <updates>
                <distro>
                    <file>training/distributor.xml</file>
                </distro>
            </updates>
        </layout>
    </adminhtml>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
<distro after="Mage_Adminhtml">Training_Distro_Adminhtml</distro>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <frontend>
        <layout>
            <updates>
                <distro>
                    <file>training/distributor.xml</file>
                </distro>
            </updates>
        </layout>
    </frontend>
    <global>
        <resources>
            <distro_setup>
                <setup>
                    <module>Training_Distro</module>
                    <class>Mage_Core_Model_Resource_Setup</class>
                </setup>
            </distro_setup>
        </resources>
        <blocks>
            <distro>
                <class>Training_Distro_Block</class>
            </distro>
        </blocks>
        <helpers>
            <distro>
                <class>Training_Distro_Helper</class>
            </distro>
        </helpers>
        <models>
            <distro>
                <class>Training_Distro_Model</class>
                <resourceModel>distro_resource</resourceModel>
            </distro>
            <distro_resource>
                <class>Training_Distro_Model_Resource</class>
                <entities>
                    <distributor>
                        <table>training_distro_entity</table>
                    </distributor>
                </entities>
            </distro_resource>
        </models>
    </global>
</config>
